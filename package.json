{
  "name": "ttd",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run src/register.ts && bun run --watch src/index.ts",
    "dev:prisma": "rm -rf src/prisma/server.db src/prisma/migratitons && prisma migrate dev --name init",
    "puppeteer": "puppeteer browsers install chrome",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "prisma": "prisma generate && prisma deploy",
    "register": "bun run src/register.ts",
    "start": "bun run src/index.ts"
  },
  "dependencies": {
    "@elysiajs/cron": "^0.7.0",
    "@prisma/client": "^5.7.0",
    "discord.js": "^14.14.1",
    "elysia": "latest",
    "puppeteer": "^21.6.1",
    "puppeteer-autoscroll-down": "^2.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/web": "^0.0.127",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "bun-types": "latest",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0"
  },
  "module": "src/index.js"
}
