generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Server {
  id       Int       @id @default(autoincrement())
  serverId String
  channels Channel[]
}

model Channel {
  id        Int     @id @default(autoincrement())
  channelId String
  active    Boolean @default(false)
  Server    Server? @relation(fields: [serverId], references: [id])
  serverId  Int?
}
